
@{
    ViewData["Title"] = "Edit";
}

@model Setting

<div class="col-10 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <form asp-action="Edit" class="forms-sample" method="post" enctype="multipart/form-data">
                <div class="form-group">
                    <label asp-for="Key">Key</label>
                    <input asp-for="Key" class="form-control" id="exampleInputName1" placeholder="Key" disabled>
                    <span asp-validation-for="Key"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Value">Value</label>
                    <input asp-for="Value" class="form-control" id="exampleInputName1" placeholder="Value">
                    <span asp-validation-for="Value"></span>
                </div>
                <button type="submit" class="btn btn-primary mr-2">Submit</button>
                <button asp-action="Index" class="btn btn-light">Cancel</button>
            </form>
        </div>
    </div>
</div>
